# Fortinet Virtual Lab Environment Variables

# General settings
COMPOSE_PROJECT_NAME=fortinet-lab

# Device counts for simulators
FORTISWITCH_COUNT=5
FORTIAP_COUNT=10
MERAKI_COUNT=5

# Brand configuration
BRANDS=arbys,bww,sonic
FORTISWITCH_BRAND=sonic
MERAKI_BRANDS=arbys,bww
FORTIAP_BRANDS=arbys,bww,sonic

# Network configuration
NETWORK_BASE=192.168.0.0
MANAGEMENT_SUBNET=192.168.0.0/24
EVE_NG_HOST=192.168.0.10

# FortiGate VM settings
FORTIGATE_CPU=2
FORTIGATE_MEMORY=4096
FORTIGATE_ADMIN_PORT=8443

# FortiManager VM settings
FORTIMANAGER_CPU=2
FORTIMANAGER_MEMORY=4096
FORTIMANAGER_PORT=541

# FortiAnalyzer VM settings
FORTIANALYZER_CPU=2
FORTIANALYZER_MEMORY=4096
FORTIANALYZER_PORT=514

# Web UI settings
UI_PORT=8080
API_GATEWAY_PORT=8000

# Logging
LOG_LEVEL=info
